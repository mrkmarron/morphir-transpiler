namespace NSMain;

function categorize(forecast: Forecast): WindCategory {
    const windCategory: WindCategory = categorizeWind(forecast.windSpeed.max);
    return windCategory;
}
function process(forecast: Forecast, inventory: CurrentInventory, reservations: ExistingReservations, returns: PendingReturns, requested: RequestedQuantity, allow: AllowPartials): Result {
    const windCategory: Result = categorizeWindForForecast(forecast);
    return decide(windCategory, forecast.shortForcast, inventory, reservations, returns, requestedQuantity, allowPartials);
}